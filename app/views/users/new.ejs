<div class="page-header">
  <h1>New person</h1>
</div>

<% form_for(user, {action: path_to.users, method: 'POST', id: "user_form"}, function (form) { %>
  <%- partial('users/form.ejs', {locals: {form: form, user: user}}) %>
  <div class="actions">
    <%- form.submit('Add person', {class: 'btn primary'}) %> or
    <%- link_to('Cancel', path_to.users, {class: 'btn'}) %>
  </div>
<% });%>